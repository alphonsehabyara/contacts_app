<h1> New contact </h1>


<%= form_tag "/contacts", method: :post do %>

  <div class="form-group">
    First Name: <input type="text" name="first_name" class="form-control">
  </div>
  <div class="form-group">
    Middle Name: <input type="text" name="middle_name" class="form-control">
  </div>
  <div class="form-control">
    Last Name: <input type="text" name="last_name" class="form-control">
  </div>
  <div class="form-control">
    Bio: <input type="text" name="bio" class="form-control">
  </div>
  <div class="form-control">
    Email: <input type="text" name="email" class="form-control">
  </div>
  <div>
  Phone Number: <input type="text" name="phone_nber" class="form-control"> 
  </div>
  <div>
    Latitude: <input type="text" name="latitude" class="form-control"> 
  </div>
  <div class="form-control">
    Longitude: <input type="text" name="longitude" class="form-control">
  </div>
  <input type="submit" name="submit" class="btn btn-info">
  </div>

<% end %>